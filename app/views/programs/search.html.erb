<% @programs.each do |program| %>
  <li class="list-group-item d-flex align-items-center py-2 px-3 rounded text-secondary"
      role="option"
      data-autocomplete-value="<%= program.title %>"
      data-autocomplete-label="<%= program.title %>"
      style="width: 20rem; cursor: pointer;">
    <%= program.title %>
  </li>
<% end %>